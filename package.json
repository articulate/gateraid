{
  "name": "gateraid",
  "version": "0.0.1",
  "description": "RAML to API Gateway tool",
  "main": "index.js",
  "dependencies": {
    "aws-sdk": "^2.2.19",
    "commander": "^2.9.0",
    "dotenv": "^1.2.0",
    "js-yaml": "^3.4.6",
    "mustache": "^2.2.1",
    "ramda": "^0.19.0",
    "raml-parser": "git+https://github.com/plukevdh/raml-js-parser.git#drop-schema-interpolation",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18"
  },
  "scripts": {
    "build": "babel --out-dir build src",
    "clean": "rm -r ./build/*"
  },
  "babel": {
    "presets": [
      "es2015"
